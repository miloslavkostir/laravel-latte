{
    "name": "miko/laravel-latte",
    "description": "Latte template engine for Laravel",
    "keywords": ["laravel", "latte", "template", "view"],
    "license": "MIT",
    "authors": [
        {
            "name": "Miloslav Koštíř",
            "email": "miloslav.kostir@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "illuminate/view": "11.x-dev",
        "latte/latte": "^3.0"
    },
    "require-dev": {
        "laravel/framework": "11.x-dev",
        "livewire/livewire": "^3.4",
        "phpunit/phpunit": "^10.0",
        "ssddanbrown/asserthtml": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Miko\\LaravelLatte\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Miko\\LaravelLatte\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Miko\\LaravelLatte\\ServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
